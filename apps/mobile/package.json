{
  "name": "mobile",
  "version": "0.1.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "echo \"(add eslint later)\" && exit 0",
    "postinstall": "patch-package && npm run bump:build",
    "bump:build": "node scripts/prepare-build-meta.js",

    "eas-build-post-install": "if [ \"$EAS_BUILD_PLATFORM\" = \"ios\" ]; then (command -v brew >/dev/null 2>&1 && (brew list cmake >/dev/null 2>&1 || brew install cmake) && (brew list ninja >/dev/null 2>&1 || brew install ninja)) || true; fi"
  },
  "engines": {
    "node": ">=20.19.4"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@supabase/supabase-js": "^2.44.4",
    "base64-arraybuffer": "^1.0.2",
    "buffer": "^6.0.3",
    "exceljs": "^4.4.0",
    "expo": "~54.0.31",
    "expo-build-properties": "^1.0.10",
    "expo-constants": "~18.0.12",
    "expo-dev-client": "~6.0.20",
    "expo-document-picker": "~14.0.8",
    "expo-file-system": "~19.0.21",
    "expo-haptics": "~15.0.8",
    "expo-image-manipulator": "~14.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linking": "~8.0.11",
    "expo-location": "~19.0.8",
    "expo-media-library": "~18.2.1",
    "expo-notifications": "~0.32.16",
    "expo-print": "~15.0.8",
    "expo-router": "~6.0.21",
    "expo-secure-store": "~15.0.8",
    "expo-sharing": "~14.0.8",
    "expo-video-thumbnails": "~10.0.8",
    "pdf-lib": "^1.17.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.52.1",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "rws-xlsx-writer": "file:modules/rws-xlsx-writer",
    "tslib": "^2.6.3",
    "xlsx": "0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/react": "~19.1.10",
    "dotenv": "^16.6.1",
